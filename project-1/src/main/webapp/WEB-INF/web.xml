<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>project-1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
   <servlet-name>LoginServlet</servlet-name>
   <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
   <servlet-name>LogoutServlet</servlet-name>
   <servlet-class>com.revature.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>HomeServlet</servlet-name>
  	<servlet-class>com.revature.servlets.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ReimbursementServlet</servlet-name>
  	<servlet-class>com.revature.servlets.ReimbursementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ReimbursementServlet</servlet-name>
  	<url-pattern>/new_reimbursement</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>PendingReimbursementServlet</servlet-name>
  	<servlet-class>com.revature.servlets.PendingReimbursementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PendingReimbursementServlet</servlet-name>
  	<url-pattern>/reimbursement_info_pending</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ResolvedReimbursementServlet</servlet-name>
  	<servlet-class>com.revature.servlets.ResolvedReimbursementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ResolvedReimbursementServlet</servlet-name>
  	<url-pattern>/reimbursement_info_resolved</url-pattern>
  </servlet-mapping>
</web-app>